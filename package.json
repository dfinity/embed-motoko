{
  "name": "embed-motoko",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "vite --port 3000",
    "build:frontend": "vite build",
    "build": "npm run build:frontend",
    "test": "vitest run",
